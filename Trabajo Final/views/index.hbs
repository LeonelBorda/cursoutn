    <!-- inicio productos -->
    <div id="productos" class="col my-3">
        <div class="container">
            <div class="row row-cols-2 lista">
                {{#each productos}}
                <div class="col"><a data-bs-toggle="modal" data-bs-target="#modal{{id}}" href=""><img src="{{imagen}}"
                alt="">{{producto}}</a></div>
                {{/each}}
